{
    "name": "lido/ada-pay-plugin",
    "description": "This is wordpress/laravel plugin for ada payments",
    "keywords": [
        "lido",
        "ada-pay-plugin"
    ],
    "homepage": "https://github.com/lido/ada-pay-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Ochara",
            "email": "michaelteddy009@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Lido\\AdaPlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lido\\AdaPlugin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
